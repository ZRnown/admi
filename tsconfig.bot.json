{
  "compilerOptions": {
    "outDir": "./dist-bot",
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "Node",
    "rootDir": "./src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": false,
    "noEmit": false,
    "strict": true,
    "lib": ["ES2021"]
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "dist-bot/**"
  ]
}
